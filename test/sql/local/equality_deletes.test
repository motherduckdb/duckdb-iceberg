require avro

require parquet

require iceberg

require httpfs

# for unpartitioned table:
# create table by spark sql: create table mytable_partitioned(id int,name string,bir date );

# First snapshot:
# insert into mytable values(1,'a',date '2025-01-01'),(2,'b',date '2025-01-02'),(3,'c',date '2025-01-03'),(4,'d',date '2025-01-04');
# Result: [(id=1, name=a, bir=2025-01-01), (id=2, name=b, bir=2025-01-02), (id=3, name=c, bir=2025-01-03), (id=4, name=d, bir=2025-01-04)]

# Second snapshot:
# delete where name=b
# Result: [(id=1, name=a, bir=2025-01-01), (id=3, name=c, bir=2025-01-03), (id=4, name=d, bir=2025-01-04)]

# Third snapshot:
# delete where id = 1
# Result: [ (id=3, name=c, bir=2025-01-03), (id=4, name=d, bir=2025-01-04)]

# Fourth snapshot:
# delete where id = 3 and name = c
# Result: [(id=4, name=d, bir=2025-01-04)]

# Fifth snapshot:
#  insert into mytable values(5,'e',date '2025-01-05'),(6,'f',date '2025-01-06');
# Result:[(id=4, name=d, bir=2025-01-04),(5,'e',date '2025-01-05'),(6,'f',date '2025-01-06')]

# Sixth snapshot:
# delete where  name = f
# Result:[(id=4, name=d, bir=2025-01-04),(5,'e',date '2025-01-05')]


# all
query III
SELECT * FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable') order by all;
----
4	d	2025-01-04
5	e	2025-01-05

# only equality delete fields
query II
SELECT id,name FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable') order by all;
----
4	d
5	e

# no equality delete fields
query I
SELECT bir FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable') order by all;
----
2025-01-04
2025-01-05

# partial equality delete field
query I
SELECT name FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable') order by all;
----
d
e

# partial equality delete field and other
query II
SELECT name,bir FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable') order by all;
----
d	2025-01-04
e	2025-01-05

# contain metadata
query II
SELECT filename[17:32], name FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable') order by all;
----
equality_deletes	d
equality_deletes	e

query I
SELECT count(*) FROM ICEBERG_SCAN('__WORKING_DIRECTORY__/data/persistent/equality_deletes/warehouse/mydb/mytable');
----
2


