# name: test/sql/local/irc/reads/test_read_nulls.test
# group: [reads]

require-env ICEBERG_SERVER_AVAILABLE

require avro

require parquet

require iceberg

require httpfs

require icu

require ducklake

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
SET TimeZone = 'UTC';

statement ok
CREATE SECRET (
    TYPE S3,
    KEY_ID 'admin',
    SECRET 'password',
    ENDPOINT '127.0.0.1:9000',
    URL_STYLE 'path',
    USE_SSL 0
);


statement ok
ATTACH '' AS my_datalake (
    TYPE ICEBERG,
    CLIENT_ID 'admin',
    CLIENT_SECRET 'password',
    ENDPOINT 'http://127.0.0.1:8181'
);


statement ok
set logging_level='debug'

query II
select * from my_datalake.default.test_null_filtering where a is null;
----
NULL	NULL
NULL	duck

query II
select * from my_datalake.default.test_null_filtering where a is not null order by a;
----
0	hello
1	world
3	NULL
5	wonderful
6	ducks
10	no nulls

query II
select * from my_datalake.default.test_null_filtering where a is distinct from null order by a;
----
0	hello
1	world
3	NULL
5	wonderful
6	ducks
10	no nulls

query II
select * from my_datalake.default.test_null_filtering where a is not distinct from null order by a;
----
NULL	NULL
NULL	duck
