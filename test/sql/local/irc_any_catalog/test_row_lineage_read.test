# name: test/sql/local/irc_any_catalog/test_row_lineage_read.test
# description: test integration with iceberg catalog read
# group: [irc_any_catalog]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
set logging_level='debug'

loop i 0 7

statement ok
set variable snapshot${i} = (
	select {
		'snapshot_id': snapshot_id,
		'timestamp_ms': timestamp_ms
	} from iceberg_snapshots(
		my_datalake.default.row_lineage_test
	) order by timestamp_ms
	offset ${i} limit 1
);

endloop

# Insert rows 0-4
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot0').snapshot_id) order by _row_id;
----
1	a
2	b
3	c
4	d
5	e

# Update row_ids 1 and 3
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot1').snapshot_id) order by _row_id;
----
1	a
2	b_u1
3	c
4	d_u1
5	e

# Delete row_ids 2 and 4
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot2').snapshot_id) order by _row_id;
----
1	a
2	b_u1
4	d_u1

# Insert 2 new rows (because of various rewrites this starts with a much higher row_id)
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot3').snapshot_id) order by _row_id;
----
1	a
2	b_u1
4	d_u1
6	f
7	g

# UPDATE row_ids 0 and 11
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot4').snapshot_id) order by _row_id;
----
1	replaced
2	b_u1
4	d_u1
6	replaced
7	g

# DELETE row_id 12
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot5').snapshot_id) order by _row_id;
----
1	replaced
2	b_u1
4	d_u1
6	replaced

# INSERT row_id 16 (again, because of various rewrites this starts with a much higher row_id)
query II
select id, data from my_datalake.default.row_lineage_test AT (VERSION=>getvariable('snapshot6').snapshot_id) order by _row_id;
----
1	replaced
2	b_u1
4	d_u1
6	replaced
7	g_new

# Latest state of the table (same as above)
query II
select id, data from my_datalake.default.row_lineage_test order by _row_id;
----
1	replaced
2	b_u1
4	d_u1
6	replaced
7	g_new
