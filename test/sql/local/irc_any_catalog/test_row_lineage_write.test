# name: test/sql/local/irc_any_catalog/test_row_lineage_write.test
# description: test integration with iceberg catalog read
# group: [irc_any_catalog]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
set logging_level='debug'

statement ok
drop table if exists my_datalake.default.duckdb_row_lineage;

statement ok
create table my_datalake.default.duckdb_row_lineage (
	id INTEGER,
	data VARCHAR
) WITH (
	'format-version' = 3
);

statement ok
insert into my_datalake.default.duckdb_row_lineage values
	(1, 'a'),
	(2, 'b'),
	(3, 'c'),
	(4, 'd'),
	(5, 'e');

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
1	0	1	a
1	1	2	b
1	2	3	c
1	3	4	d
1	4	5	e

statement ok
UPDATE my_datalake.default.duckdb_row_lineage
SET data = data || '_u1'
WHERE id IN (2, 4);

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
1	0	1	a
2	1	2	b_u1
1	2	3	c
2	3	4	d_u1
1	4	5	e

statement ok
DELETE FROM my_datalake.default.duckdb_row_lineage
WHERE id IN (3, 5);

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
1	0	1	a
2	1	2	b_u1
2	3	4	d_u1

statement ok
insert into my_datalake.default.duckdb_row_lineage values
	(6, 'f'),
	(7, 'g');

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
1	0	1	a
2	1	2	b_u1
2	3	4	d_u1
4	7	6	f
4	8	7	g

statement ok
UPDATE my_datalake.default.duckdb_row_lineage
SET data = 'replaced'
WHERE id IN (1, 6);

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
5	0	1	replaced
2	1	2	b_u1
2	3	4	d_u1
5	7	6	replaced
4	8	7	g

statement ok
DELETE FROM my_datalake.default.duckdb_row_lineage
WHERE id = 7;

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
5	0	1	replaced
2	1	2	b_u1
2	3	4	d_u1
5	7	6	replaced

statement ok
insert into my_datalake.default.duckdb_row_lineage values
	(7, 'g_new');

query IIII
select sequence_number, rowid, id, data from my_datalake.default.duckdb_row_lineage order by rowid;
----
5	0	1	replaced
2	1	2	b_u1
2	3	4	d_u1
5	7	6	replaced
7	11	7	g_new
