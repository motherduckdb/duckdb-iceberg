# name: test/sql/local/irc_any_catalog/insert/test_transactional_insert_v3.test
# description: test integration with iceberg catalog read
# group: [insert]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
set enable_logging=true

statement ok
set logging_level='debug'

statement ok
drop table if exists my_datalake.default.tbl;

statement ok
create table my_datalake.default.tbl (
	id INTEGER,
	data VARCHAR
) WITH (
	'format-version' = 3
);

statement ok
begin transaction;

# Insert into an empty table
statement ok
insert into my_datalake.default.tbl values
	(1, 'a'),
	(2, 'b'),
	(3, 'c'),
	(4, 'd'),
	(5, 'e');

# Scan the transaction-local snapshot/manifest_file/manifest_entry we created
query IIII
select sequence_number, rowid, id, data from my_datalake.default.tbl order by rowid;
----
1	0	1	a
1	1	2	b
1	2	3	c
1	3	4	d
1	4	5	e

# Do another insert
statement ok
insert into my_datalake.default.tbl values
	(6, 'f'),
	(7, 'g')

# Scan the transaction-local snapshot/manifest_file/manifest_entry we created
query IIII
select sequence_number, rowid, id, data from my_datalake.default.tbl order by rowid;
----
1	0	1	a
1	1	2	b
1	2	3	c
1	3	4	d
1	4	5	e
2	5	6	f
2	6	7	g

statement ok
commit

# After we commit, this should give the same result
query IIII
select sequence_number, rowid, id, data from my_datalake.default.tbl order by rowid;
----
1	0	1	a
1	1	2	b
1	2	3	c
1	3	4	d
1	4	5	e
2	5	6	f
2	6	7	g
