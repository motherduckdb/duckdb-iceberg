# name: test/sql/local/test_iceberg_and_ducklake.test
# description: test integration with iceberg catalog read
# group: [local]

require-env ICEBERG_SERVER_AVAILABLE

require avro

require parquet

require iceberg

require httpfs

require ducklake

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
pragma threads=1;

statement ok
CALL enable_logging('HTTP');

statement ok
set logging_level='debug';

statement ok
CREATE SECRET (
    TYPE S3,
    KEY_ID 'admin',
    SECRET 'password',
    ENDPOINT '127.0.0.1:9000',
    URL_STYLE 'path',
    USE_SSL 0
);


statement ok
ATTACH '' AS my_datalake (
    TYPE ICEBERG,
    CLIENT_ID 'admin',
    CLIENT_SECRET 'password',
    ENDPOINT 'http://127.0.0.1:8181'
);

statement ok
ATTACH 'ducklake:duckdb:ducklake.duckdb' as my_ducklake (DATA_PATH 'data_path');

# this output will change if more tests add schemas.
query II
select request.url, request.type from duckdb_logs_parsed('HTTP');
----
http://127.0.0.1:8181/v1/oauth/tokens	POST
http://127.0.0.1:8181/v1/config?warehouse=	GET
http://127.0.0.1:8181/v1/namespaces	GET
http://127.0.0.1:8181/v1/namespaces/default/tables	GET
http://127.0.0.1:8181/v1/namespaces/level1/tables	GET
